{
  "identifier": "mobile-capability",
  "windows": ["main"],
  "platforms": ["iOS", "android"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "file-picker-android:allow-pickFiles",
    "barcode-scanner:allow-scan",
    "barcode-scanner:allow-cancel",
    "os:allow-platform"
  ]
}
