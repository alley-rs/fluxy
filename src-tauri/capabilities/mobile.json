{
  "identifier": "mobile-capability",
  "windows": ["main"],
  "platforms": ["iOS", "android"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "file-picker-android:allow-pickFiles",
    "barcode-scanner:allow-scan",
    "barcode-scanner:allow-cancel",
    "os:allow-platform"
  ]
}
